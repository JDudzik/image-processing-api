{
  "name": "image-processing-api",
  "version": "1.0.0",
  "main": "build/server.js",
  "description": "This project is for a Udacity mentor certification on their `Image Processing API`.",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "npm run build:live",
    "start:prod": "node ./build/server.js",
    "build": "tsc",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/server.ts",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint \"src/**\" --fix",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JDudzik/image-processing-api.git"
  },
  "author": "Josiah Dudzik",
  "homepage": "https://github.com/JDudzik/image-processing-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.6"
  }
}
